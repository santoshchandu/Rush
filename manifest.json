{
  "manifest_version": 3,
  "name": "Rush",
  "version": "1.0",
  "description": "Rush through videos at high speeds",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
