{
  "manifest_version": 3,
  "name": "Rush",
  "version": "1.0",
  "description": "Rush through videos at high speeds",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://www.netflix.com/*",
    "https://vimeo.com/*",
    "https://www.twitch.tv/*",
    "https://www.hulu.com/*",
    "https://www.amazon.com/*",
    "https://www.disneyplus.com/*",
    "https://www.hbo.com/*",
    "https://www.peacocktv.com/*",
    "https://www.crunchyroll.com/*",
    "https://www.udemy.com/*",
    "https://www.coursera.org/*",
    "https://www.linkedin.com/*",
    "https://www.skillshare.com/*",
    "https://www.pluralsight.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://www.netflix.com/*",
        "https://vimeo.com/*",
        "https://www.twitch.tv/*",
        "https://www.hulu.com/*",
        "https://www.amazon.com/*",
        "https://www.disneyplus.com/*",
        "https://www.hbo.com/*",
        "https://www.peacocktv.com/*",
        "https://www.crunchyroll.com/*",
        "https://www.udemy.com/*",
        "https://www.coursera.org/*",
        "https://www.linkedin.com/*",
        "https://www.skillshare.com/*",
        "https://www.pluralsight.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
